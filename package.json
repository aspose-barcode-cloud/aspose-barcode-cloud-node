{
  "name": "aspose-barcode-cloud-node",
  "version": "20.10.0",
  "description": "Aspose.BarCode Cloud SDK for Node.js",
  "homepage": "https://products.aspose.cloud/barcode/nodejs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aspose-barcode-cloud/aspose-barcode-cloud-node.git"
  },
  "keywords": [
    "Aspose",
    "Cloud",
    "BarCode"
  ],
  "author": "Aspose.BarCode Team",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsc --build",
    "test": "tsdx test",
    "cover": "tsdx test --collect-coverage",
    "lint": "tsc --strict",
    "format": "tsdx lint --fix",
    "prepare": "tsdx build --target=node",
    "clear": "rm dist/* || DEL /S /Q dist",
    "check-updates": "ncu -u"
  },
  "peerDependencies": {},
  "prettier": {
    "printWidth": 120,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4
  },
  "devDependencies": {
    "@types/uuid": "^8.3.0",
    "npm-check-updates": "^9.2.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.0.3",
    "typescript": "^4.0.5",
    "uuid": "^8.3.1"
  },
  "dependencies": {
    "@types/request": "^2.48.5",
    "request": "^2.88.2"
  },
  "bugs": {
    "url": "https://github.com/aspose-barcode-cloud/aspose-barcode-cloud-node/issues"
  },
  "license": "MIT",
  "readmeFilename": "README.md",
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/*.d.ts"
    ]
  }
}
